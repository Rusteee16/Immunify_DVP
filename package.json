    "@dfinity/auth-client": "^0.15.4",
    "@dfinity/authentication": "^0.14.2",
    "@dfinity/identity": "^0.15.4",
    "@svgr/webpack": "^7.0.0",
    "bootstrap": "^5.2.3",
    "image-webpack-loader": "^8.1.0",
    "lit-html": "^2.7.0",
    "lodash": "^4.17.21",
